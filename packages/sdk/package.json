{
  "name": "tuturuuu",
  "version": "0.1.4",
  "description": "Official TypeScript/JavaScript SDK for the Tuturuuu platform",
  "license": "MIT",
  "author": "Tuturuuu",
  "homepage": "https://tuturuuu.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/tutur3u/platform",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/tutur3u/platform/issues"
  },
  "keywords": ["tuturuuu", "sdk", "api"],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "import": "./dist/storage.js",
      "require": "./dist/storage.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js",
      "require": "./dist/errors.js"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json --watch --incremental",
    "type-check": "tsgo --project tsconfig.build.json",
    "clean": "rm -rf dist .turbo",
    "prepublishOnly": "bun run clean && bun run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@tuturuuu/types": "0.2.3",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@tuturuuu/typescript-config": "workspace:*",
    "@types/node": "^25.0.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "bun@1.3.4"
}
