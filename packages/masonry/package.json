{
  "name": "@tuturuuu/masonry",
  "version": "0.3.10",
  "type": "module",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json --watch --incremental",
    "type-check": "tsgo --project tsconfig.build.json",
    "clean": "rm -rf dist .turbo",
    "prepublishOnly": "bun run clean && bun run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@tuturuuu/typescript-config": "workspace:*",
    "@types/node": "^25.0.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "jsdom": "^27.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist", "README.md"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./masonry": {
      "types": "./dist/masonry.d.ts",
      "import": "./dist/masonry.js",
      "require": "./dist/masonry.js"
    }
  },
  "packageManager": "bun@1.3.6"
}
