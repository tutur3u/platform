{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "github": {
    "enabled": false
  },
  "crons": [
    {
      "path": "/api/cron/discord/wol/daily/remind",
      "schedule": "0 14 * * *"
    },
    {
      "path": "/api/cron/discord/daily-report",
      "schedule": "0 16 * * *"
    },
    {
      "path": "/api/cron/tasks/generate-embeddings",
      "schedule": "0 2 * * *"
    }
  ],
  "functions": {
    "src/app/api/**/*": {
      "maxDuration": 300
    }
  }
}
