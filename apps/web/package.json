    "start": "next start",
    "preview": "next build && next start -p 7803 --turbo",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "stop": "pnpm supabase stop",
