  "scripts": {
    "dev": "next dev -p 7803 --turbo",
    "devx": "pnpm supabase stop && pnpm i && pnpm supabase start && next dev -p 7803 --turbo",
    "bdevx": "bun supabase stop && bun i && bun supabase start && next dev -p 7803 --turbo",
    "devrs": "pnpm supabase stop --no-backup && pnpm i && pnpm supabase start && pnpm supabase db reset && next dev -p 7803 --turbo",
