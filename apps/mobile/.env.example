# Mobile app environment variables
# Copy this file to .env and fill in your values:
#   cp .env.example .env
#
# Then run with:
#   flutter run --dart-define-from-file=.env -t lib/main_development.dart

# Supabase project URL
# Local: http://localhost:54321
# Physical device: use your Mac's local IP, e.g. http://192.168.1.100:54321
NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL

# Supabase anonymous (public) key from `bun sb:start` output
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=YOUR_SUPABASE_PUBLISHABLE_KEY

# Web app API base URL (serves /api/v1/auth/mobile/* endpoints)
# Local: http://localhost:7803
# Physical device: use your Mac's local IP, e.g. http://192.168.1.100:7803
API_BASE_URL=http://localhost:7803

# Cloudflare Turnstile
TURNSTILE_BASE_URL=YOUR_TURNSTILE_BASE_URL
TURNSTILE_SITE_KEY=YOUR_TURNSTILE_SITE_KEY
