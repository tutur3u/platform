{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "preserveSymlinks": true,
    "paths": {
      "@/*": ["./*"],
      "@tuturuuu/types": ["./types/tuturuuu-types"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
