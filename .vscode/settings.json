{
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit",
    "source.fixAll.biome": "explicit",
    "source.fixAll": "explicit"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/flutter/ephemeral": true,
    "**/Flutter/ephemeral": true,
    "**/.symlinks": true,
    "**/.plugin_symlinks": true,
    ".agent": true,
    ".agents": true,
    ".claude": true,
    ".codex": true,
    ".continue": true,
    ".cursor": true,
    ".gemini": true,
    "**/.idea": true,
    ".idx": true,
    ".junie": true,
    ".opencode": true,
    ".trae": true,
    ".zed": true,
    ".windsurf": true,
    "**/.dart_tool": true,
    "**/.next": true,
    "**/__pycache__": true,
    "**/.venv": true,
    "**/*_cache": true,
    // "**/build": true,
    "**/*.tsbuildinfo": true,
    "conductor": true,
    "skills": true,
    "CODE_OF_CONDUCT.md": true,
    "CONTRIBUTING.md": true,
    "AGENTS.md": true,
    "CLAUDE.md": true,
    "GEMINI.md": true,
    "WARP.md": true,
    "SECURITY.md": true,
    "LICENSE": true,
    "**/node_modules": true,
    "**/coverage": true,
    "**/.turbo": true
  },
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.defaultFormatter": "biomejs.biome",
  "[xml]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[css]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "workbench.editor.customLabels.patterns": {
    "**/app/**/page.tsx": "${dirname} - Page",
    "**/app/**/layout.tsx": "${dirname} - Layout",
    "**/app/**/route.ts": "${dirname} - API Route",
    "**/components/**/index.tsx": "${dirname} - Component"
  },
  "tailwindCSS.experimental.configFile": "packages/ui/src/globals.css",
  "i18n-ally.enabledFrameworks": ["next-intl"],
  "i18n-ally.localesPaths": [
    "apps/calendar/messages",
    "apps/web/messages",
    "apps/rewise/messages",
    "apps/nova/messages"
  ],
  "i18n-ally.autoDetection": true,
  "i18n-ally.keystyle": "nested",
  "codeQL.githubDatabase.update": "never",
  "codeQL.githubDatabase.download": "never",
  "IDX.aI.enableInlineCompletion": true,
  "IDX.aI.enableCodebaseIndexing": true,
  "i18n-ally.sourceLanguage": "en",
  "typescript.experimental.useTsgo": true,
  "cSpell.words": ["Tuturuuu", "tuturuuu"],
  "python-envs.defaultEnvManager": "ms-python.python:venv",
  "python-envs.defaultPackageManager": "ms-python.python:pip",
  "python-envs.pythonProjects": [],
  "vitest.disableWorkspaceWarning": true
}
